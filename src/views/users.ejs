<%- include("./partials/head") %>
<main>
  <h1>Usuários</h1>
  <!-- ? Pegar variável users e mapear os usuários.  -->
  <!-- ? Deve imprimir o nome, sobrenome e idade  -->
  <!-- ? Tempo: 10 min  -->
  <!-- todo: Finalizamos as 21h37  -->
  <!-- ? Caso não haja usuário, deve imprimir:  -->
  <!-- ? "Nenhum usuário encontrado"  -->
  <% if(users.length > 0) {%> 
    <% for(let user of users){ %>
      <div>
        <h2><%= user.nome %></h2>
        <h2><%= user.sobrenome %></h2>
        <h3><%= user.idade %></h3> 
      </div>
    <% }%>
  <% } else {%>
    <h1>Nenhum usuário encontrado</h1>
  <% } %>
</main>
<%- include("./partials/footer") %>
